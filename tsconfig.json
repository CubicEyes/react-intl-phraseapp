  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "jsx": "react",
    "module": "commonjs",
    "declaration": false,
    "moduleResolution": "node",
    "allowJs": true
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}